package sec1;

public class Example3 {

	public static void main(String[] args) {
		// 부호 연산자 및 증감 연산자
		
		int a = 20;
		int b = 30;
		int c = a - b;
		System.out.println("c="+c);
		int d = 1;
		System.out.println("e="+c*-d);
		d = c * -1;
		System.out.println("e="+c*-d);
				
		a = 10;
		b = 10;
		System.out.println(++a);
		System.out.println(b++);
		System.out.println(a);
	}
}