package sec4;

public class Q5 {

	public static void main(String[] args) {
		// 순서대로 정렬 (selection sort)
		int[] arr = {70, 50, 90, 80, 40, 60};
		
		for (int i=0; i<5; i++){
			for(int j=1; j<6; j++){
				if (arr[i]>arr[j]){
					int tmp = arr[i];
					arr[i] = arr[j];
					arr[j] = tmp;
				}
			}	
			System.out.print(arr[i]);
		}
	}
}