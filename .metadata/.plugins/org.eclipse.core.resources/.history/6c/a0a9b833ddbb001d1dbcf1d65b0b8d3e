package sec1;

public class Example3 {

	public static void main(String[] args) {
		// 
		int su1 = 1004;			//10진수
		int su2 = 0b1101;		//2진수
		int su3 = 01004;		//8진수
		int su4 = 0x1004;		//16진수
		
		System.out.println("su1="+su1);
		System.out.println("su2="+su2);
		System.out.println("su3="+su3);
		System.out.println("su4="+su4);

	}

}
