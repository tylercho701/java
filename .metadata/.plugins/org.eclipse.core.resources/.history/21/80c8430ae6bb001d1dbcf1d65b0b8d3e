package sec1;

public class Example2 {
	public static void main(String[] args) {
		// java의 기본 자료형(primitive)_변수는 반드시 추기화(값을 지정해 주는 행위) 되어야 한다.
		// byte, boolean, short, int, long, float, double, char
		// 기본 자료형(primitive) : 하나의 자료만 저정할 수 있음.
		// String
		
		byte a;
		int b;
		short c;
		double d;
		boolean e;
		long f;
		float g;
		char h;
	}
}