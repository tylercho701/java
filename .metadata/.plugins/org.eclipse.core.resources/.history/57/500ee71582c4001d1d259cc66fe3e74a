package sec4;

public class Q5 {

	public static void main(String[] args) {
		// 순서대로 정렬
		int[] arr = {70, 50, 90, 80, 40, 60};
		
		int[] odr = {1, 1, 1, 1, 1, 1};
		for (int i=0; i<arr.length; i++){
			for(int j=0; j<arr.length; j++){
				if (arr[i]>arr[j]){
					odr[i]++;
				}
			}
		}System.out.printf("%#x", *arr[i]);
		;
	}
}