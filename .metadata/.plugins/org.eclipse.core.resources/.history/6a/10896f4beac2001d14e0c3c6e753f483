package exercise;

public class Q4 {

	public static void main(String[] args) {
		// Q4
		
		int i = 0;
		int j = 0;
		
		while (i+j!=5) {
			i = (int) (Math.random()*6)+1;
			j = (int) (Math.random()*6)+1;
			System.out.println("i + j = "+"( "+i+", "+j+" )");
		}
		System.out.println("주사위 게임 종료");
	}
}