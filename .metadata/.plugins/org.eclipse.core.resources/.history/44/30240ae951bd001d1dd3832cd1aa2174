package sec2;

public class Example3 {

	public static void main(String[] args) {
		// 형 변환의 예1
		int kor = 80;
		int eng = 70;
		int mat = 95;
		int tot = kor = eng = mat;
		
		System.out.println("국어 : "+kor);
		System.out.println("영어 : "+eng);
		System.out.println("수학 : "+mat);
		System.out.println("총점 : "+tot);
	}

}
