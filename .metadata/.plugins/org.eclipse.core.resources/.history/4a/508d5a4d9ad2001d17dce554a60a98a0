package sec1;

import java.util.ArrayList;

public class ArrayListEx1 {

	public static void main(String[] args) {
		ArrayList<Integer> jum = new ArrayList<>();
		
		jum.add(100);
		jum.add(95);
		jum.add(94);
		jum.add(98);
		
		for(int i=0; i<jum.size(); i++){
			int a = jum.get(i);
			System.out.println(a);
		}
	}
}