package sec2;

public class Practice {

	public static void main(String[] args) {
		// 연습문제.7
		
		int[] array = {1, 5, 3, 8, 2 };
		int max = 0;
		for (int i = 0; i<array.length; i++) {
			if (array[i]>max){
				max = array[i]}
			}		// 최대값 변수를 우선 선언 후 array.length와 일일이 대조하게 한다.
		System.out.println();
	}
}