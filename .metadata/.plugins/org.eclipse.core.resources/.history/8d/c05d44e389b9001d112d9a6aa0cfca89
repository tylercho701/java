package ch01.sec11;

public class Test1 {

	public static void main(String[] args) {
		//변수타입 변수명;		---		변수 선언
		//변수명=변수값;		---		변수값 설정
		//변수명 값;		---		초기화
		//변수타입 변수명=값;		---		변수 선언 + 초기화
		//변수타입 변수명1,변수명2,변수명3;		---		여러 변수의 선언
		//변수타입 변수명1=값,변수명2=값,변수명3=값;		---		여러 변수의 선언
		
		//변수타입(기본자료형)
		//숫자 : 정수, 실수
		//정수 : byte(1), short(2), int(4), long(8)
		//실수 : float(4), double(8)
		
		//문자 : 1문자, 문자열
		//문자 : char(2), String(?)/가변문자열
		//논리 : bool(1)

	}

}
