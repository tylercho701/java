package practice1;

import java.util.Scanner;

public class Prog {

	public static void main(String[] args) {
		// 계좌 입출금 관리 프로그램 (ch6-20)
		Scanner num = new Scanner();
		int sw = 0;
		
		while sw:
		System.out.println("---------------------------------------");
		System.out.println("1.계좌생성 | 2.계좌목록 | 3.예금 | 4.출금 | 5.종료");
		System.out.println("---------------------------------------");
		System.out.println("선택>"+num);
		System.out.println("-----------");
	}
}