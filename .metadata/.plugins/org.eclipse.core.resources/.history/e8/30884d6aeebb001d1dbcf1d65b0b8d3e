package sec1;

public class ExampleC {

	public static void main(String[] args) {
		//String : 문자열 자료형
		// \(역슬래쉬) : 입츨력 제어 문자
		// \n 줄바꿈, \t tab(4칸 띄우기), /" 쌍따옴표
		
		String a = "조";
		String b = "조교행\n안녕/tTYLER";
		String c = "d:\\tyler\\java";
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}
}