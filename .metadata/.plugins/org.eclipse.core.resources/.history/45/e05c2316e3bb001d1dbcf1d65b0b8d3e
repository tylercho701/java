package sec1;

public class Example5 {

	public static void main(String[] args) {
		// Short
		short a = 1234;
		short b = 4321;
		short c = -30000;
		short d = 32000;
		short e = -0;
		byte f = 124;
		
		System.out.println("a = "+a);
		System.out.println("b = "+b);
		System.out.println("c = "+c);
		System.out.println("d = "+d);
		System.out.println("e = "+e);
		System.out.println("f = "+f);
	}
}